[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.3

[libraries]
linux.debug.x86_64 =     "res://rust/target/debug/libbitedrust.so"
linux.release.x86_64 =   "res://rust/target/release/libbitedrust.so"
windows.debug.x86_64 =   "res://rust/target/debug/bitedrust.dll"
windows.release.x86_64 = "res://rust/target/release/bitedrust.dll"
macos.debug =            "res://rust/target/debug/libbitedrust.dylib"
macos.release =          "res://rust/target/release/libbitedrust.dylib"
macos.debug.arm64 =      "res://rust/target/debug/libbitedrust.dylib"
macos.release.arm64 =    "res://rust/target/release/libbitedrust.dylib"

config_version=5

[application]

config/name="bited"
config/description="	"
run/main_scene="res://components/start/start.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="bited"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/image="res://assets/splash/splash.png"
config/icon="res://assets/icons/icon.svg"
config/macos_native_icon="res://assets/icons/icon.icns"
config/windows_native_icon="res://assets/icons/icon.ico"

[autoload]

StateVars="*res://components/VARS/state_vars.gd"
StyleVars="*res://components/VARS/style_vars.gd"
Util="*res://components/util.gd"
Thumb="*res://components/table/thumb.tscn"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1200
window/size/viewport_height=675
window/size/borderless=true
window/energy_saving/keep_screen_on=false
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="bited"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-sqlite/plugin.cfg")

[gui]

timers/tooltip_delay_sec=0.1
theme/custom="res://components/dark.tres"
theme/custom_font="res://assets/bited.woff2"
theme/default_font_antialiasing=0
theme/default_font_hinting=0
theme/default_font_subpixel_positioning=0

